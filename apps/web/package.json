{
  "name": "@documents-manager/web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "preview": "npm run build && vite preview",
    "deploy": "npm run build && wrangler deploy",
    "build:staging": "react-router build --mode staging",
    "build:production": "react-router build --mode production",
    "deploy:staging": "npm run build:staging && wrangler deploy",
    "deploy:production": "npm run build:production && wrangler deploy",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate:stg": "npx dotenv -e .env.staging -- npm run db:generate && npx wrangler d1 migrations apply DB --remote --env staging",
    "cf-typegen": "npx wrangler types --env staging",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.3",
    "fast-xml-parser": "^5.2.5",
    "lucide-react": "^0.525.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "7.7.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.5",
    "windows-1251": "^3.0.4"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.9.6",
    "@react-router/dev": "^7.7.0",
    "@react-router/node": "^7.7.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/xml2js": "^0.4.14",
    "@vitejs/plugin-react": "^4.6.0",
    "dotenv": "^17.2.0",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.4",
    "install": "^0.13.0",
    "npm": "^11.4.2",
    "vite": "^7.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.0",
    "wrangler": "^4.25.0"
  }
}
